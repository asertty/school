public class Random {

	public static void main(String[] args) {
		
		//int 숫자= 1;
		//int 숫자2=1;
		//while(숫자2<=10) {
		//	숫자 = 1;
			
			for (int a = 1; a<=5; a++) {
				System.out.print("a:"+a+" ");
				for (int b = 1; b<=a; b++) {
					System.out.print("*");
				}
			System.out.println();
			
		}
		
		}
	}
