package main;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		//가로 세로 입력 받기
		Scanner sc = new Scanner(System.in);
		
		System.out.print("가로 = ");
		int 가로 = sc.nextInt();
		
		System.out.print("세로 = ");
		int 세로 = sc.nextInt();
		
		//계산 
		int 넓이 = 넓이계산(가로, 세로);
		
		System.out.println("넓이 = "+넓이);
	}	
		
		// 값 출력 
	static int 넓이계산(int 숫자1, int 숫자2) {
		
		int result = 숫자1 * 숫자2;
		return result;
	}

}
